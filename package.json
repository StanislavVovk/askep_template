{
  "name": "askep_template_processor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint": "npx eslint --ext .js,jsx,ts,tsx src --fix",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
